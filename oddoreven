import java.util.*;
import java.lang.*;
class oddoreven
{
 public static void main(String args[])
 {
   int no;
   Scanner in  = new Scanner(System.in);
   no = in.nextInt();
   if(no%2==0)
    System.out.println("Even");
   else if(n%2!=0)
    System.out.println("Odd");
   else 
    System.out.println("Invalid");
 }
}
